#################################################################
#
#    Externals for visualization
#
#################################################################

schema:Place  
        a                 owl:Class ;
        rdfs:comment      "Entities that have a somewhat fixed, physical extension."@en ;
        rdfs:isDefinedBy  schema: ;
        rdfs:label        "Place"@en .

schema:CivicStructure  
        a                 owl:Class ;
        rdfs:comment      "A public structure, such as a town hall or concert hall, parking facility or charging point."@en ;
        rdfs:isDefinedBy  schema: ;
        rdfs:label        "Civic Structure"@en , "Zivile Struktur"@de ;
        rdfs:subClassOf   schema:Place .

mv:ParkingFacility  
        a                    owl:Class , rdfs:Class ;
        rdfs:comment         "Any facility or area assigned for parking vehicles. A parking facility can provide one or more parking spaces."@en ;
        rdfs:isDefinedBy     mv: ;
        rdfs:label           "Parking Facility"@en , "Parkeinrichtung"@de ;
        rdfs:subClassOf      schema:CivicStructure .

mv:BicycleParkingStation
        a                 owl:Class , rdfs:Class ;
        rdfs:comment      "Building or structure designed for use as a bicycle parking facility."@en ;
        rdfs:isDefinedBy  mv: ;
        rdfs:label        "Bicycle Parking Station"@en , "Fahrradparkstation"@de ;
        rdfs:subClassOf   mv:ParkingFacility .

schema:amenityFeature  
        a                 owl:ObjectProperty ;
        rdfs:comment      "An amenity feature (e.g. a characteristic or service) of a place or accomodation."@en ;
        rdfs:domain       schema:Place ;
        rdfs:isDefinedBy  schema: ;
        rdfs:label        "amenity feature"@en ;
        rdfs:range        schema:LocationFeatureSpecification .

schema:LocationFeatureSpecification  
        a                 owl:Class ;
        rdfs:comment      "Specifies a location feature by providing a structured value representing a feature of an accommodation as a property-value pair of varying degrees of formality."@en ;
        rdfs:isDefinedBy  schema: ;
        rdfs:label        "LocationFeatureSpecification"@en  .

schema:Vehicle  
        a                 owl:Class ;
        rdfs:comment      "A vehicle is a device that is designed or used to transport people or cargo over land, water, air, or through space."@en ;
        rdfs:isDefinedBy  schema: ;
        rdfs:label        "Vehicle"@en , "Fahrzeug"@de .

schema:PostalAddress  
        a                 owl:Class ;
        rdfs:comment      "The mailing address."@en ;
        rdfs:isDefinedBy  schema: ;
        rdfs:label        "Postal Address"@en .

schema:address  
        a                 owl:ObjectProperty ;
        rdfs:comment      "Physical address of the item."@en ;
        rdfs:domain       schema:Place ;
        rdfs:isDefinedBy  schema: ;
        rdfs:label        "address"@en ;
        rdfs:range        schema:PostalAddress .

gr:BusinessEntity  
        a                 owl:Class ;
        rdfs:comment      "An instance of this class represents the legal agent making (or seeking) a particular offering. This can be a legal body or a person. A business entity has at least a primary mailing address and contact details. For this, typical address standards (vCard) and location data (geo, WGS84) can be attached. Note that the location of the business entity is not necessarily the location from which the product or service is being available."@en ;
        rdfs:isDefinedBy  gr: ;
        rdfs:label        "Business Entity"@en , "Juristische Person"@de .

mv:ownedBy  
        a                 owl:ObjectProperty , rdf:Property ;
        rdfs:comment      "The owner of a civic structure or facility. Owner and operator can be different business entities."@en ;
        rdfs:domain       schema:CivicStructure ;
        rdfs:isDefinedBy  mv: ;
        rdfs:label        "owned by"@en , "in Besitz von"@de ;
        rdfs:range        gr:BusinessEntity .

mv:operatedBy  
        a                 rdf:Property , owl:ObjectProperty ;
        rdfs:comment      "The operator of a civic structure or facility. Operator and owner can be different business entitites."@en ;
        rdfs:domain       schema:CivicStructure ;
        rdfs:isDefinedBy  mv: ;
        rdfs:label        "operated by"@en , "betrieben von"@de ;
        rdfs:range        gr:BusinessEntity .

dct:identifier  
        a                 rdf:Property , owl:DatatypeProperty ;
        rdfs:comment      "An unambiguous reference to the resource within a given context."@en ;
        rdfs:domain       mv:BicycleParkingStation ;
        rdfs:isDefinedBy  dct: ;
        rdfs:label        "identifier"@en ;
        rdfs:range        xsd:string .

schema:name  
        a                 rdf:Property , owl:DatatypeProperty ;
        rdfs:comment      "The name of an item."@en ;
        rdfs:domain       schema:Place ;
        rdfs:isDefinedBy  schema: ;
        rdfs:label        "name"@en ;
        rdfs:range        xsd:string .

mv:totalCapacity  
        a                 owl:DatatypeProperty , rdf:Property ;
        rdfs:comment      "Indicates the total number of parking spaces of a parking facility." ;
        rdfs:domain       mv:ParkingFacility ;
        rdfs:isDefinedBy  mv: ;
        rdfs:label        "total capacity"@en , "Gesamtkapazität"@de ;
        rdfs:range        xsd:integer .

mv:Capacity  
        a                 owl:Class , rdfs:Class ;
        rdfs:comment      "Capacity of a civic structure."@en ;
        rdfs:isDefinedBy  mv: ;
        rdfs:label        "Capacity"@en , "Kapazität"@de .

mv:capacity  
        a                 rdf:Property , owl:ObjectProperty ;
        rdfs:comment      "Relates the civic structure to its capacity."@en ;
        rdfs:domain       schema:CivicStructure ;
        rdfs:isDefinedBy  mv: ;
        rdfs:label        "has capacity"@en , "hat Kapazität"@de ;
        rdfs:range        mv:Capacity .

mv:RealTimeCapacity  
        a                 owl:Class , rdfs:Class ;
        rdfs:comment      "Real-time capacity of a parking facility or parking space."@en , "Real-time capacity of a civic structure."@en ;
        rdfs:isDefinedBy  mv: ;
        rdfs:label        "Real Time Capacity"@en , "Echtzeit Kapazität"@de ;
        rdfs:subClassOf   mv:Capacity .

mv:currentValue  
        a                 owl:DatatypeProperty , rdf:Property ;
        rdfs:comment      "Indicates the current value of the real-time capacity."@en ;
        rdfs:domain       mv:RealTimeCapacity ;
        rdfs:isDefinedBy  mv: ;
        rdfs:label        "current value"@en ;
        rdfs:range        xsd:integer .

dct:date  
        a                 owl:DatatypeProperty , rdf:Property ;
        rdfs:comment      "The date of the given real-time capacity."@en ;
        rdfs:domain       mv:RealTimeCapacity ;
        rdfs:isDefinedBy  dct: ;
        rdfs:label        "date"@en ;
        rdfs:range        xsd:date .

schema:geo
        a                 rdf:Property , owl:ObjectProperty ;
        rdfs:comment      "The geo coordinates of a place."@en ;
        rdfs:domain       schema:Place ;
        rdfs:isDefinedBy  schema: ;
        rdfs:label        "geo"@en ;
