        rdfs:isDefinedBy  schema: ;
        rdfs:label        "Geo Shape"@en .

schema:hasMap
        a                 owl:ObjectProperty , rdf:Property ;
        rdfs:comment      "A URL to a map of the place."@en ;
        rdfs:domain       schema:Place ;
        rdfs:isDefinedBy  schema: ;
        rdfs:label        "has map"@en ;
        rdfs:range        schema:Map .

schema:Map  
        a                 owl:Class , rdfs:Class ;
        rdfs:comment      "A map."@en ;
        rdfs:isDefinedBy  schema: ;
        rdfs:label        "Map"@en .

dbo:closeTo
        a                 owl:ObjectProperty , rdf:Property ;
        rdfs:comment      "Place close to another place."@en ;
        rdfs:domain       schema:Place ;
        rdfs:isDefinedBy  dbo: ;
        rdfs:label        "close to"@en ;
        rdfs:range        schema:Place .

schema:contactPoint
        a                 owl:ObjectProperty , rdf:Property ;
        rdfs:comment      "A contact point for a person or organization."@en ;
        rdfs:domain       mv:BicycleParkingStation ;
        rdfs:isDefinedBy  schema: ;
        rdfs:label        "contact point"@en ;
        rdfs:range        schema:ContactPoint .

schema:ContactPoint
        a                 owl:Class , rdfs:Class ;
        rdfs:comment      "A contact point for customer service."@en ;
        rdfs:isDefinedBy  schema: ;
        rdfs:label        "Contact Point"@en .

schema:contactType
